<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>AddStudentController</servlet-name>
  	<display-name>AddStudentController</display-name>
  	<description></description>
  	<servlet-class>com.haresh.controllers.AddStudentController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AllStudentsController</servlet-name>
  	<display-name>AllStudentsController</display-name>
  	<description></description>
  	<servlet-class>com.haresh.controllers.AllStudentsController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteStudentController</servlet-name>
  	<display-name>DeleteStudentController</display-name>
  	<description></description>
  	<servlet-class>com.haresh.controllers.DeleteStudentController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EditStudentController</servlet-name>
  	<display-name>EditStudentController</display-name>
  	<description></description>
  	<servlet-class>com.haresh.controllers.EditStudentController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetStudentController</servlet-name>
  	<display-name>GetStudentController</display-name>
  	<description></description>
  	<servlet-class>com.haresh.controllers.GetStudentController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AddStudentController</servlet-name>
  	<url-pattern>/AddStudent</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AllStudentsController</servlet-name>
  	<url-pattern>/AllStudents</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteStudentController</servlet-name>
  	<url-pattern>/DeleteStudent</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EditStudentController</servlet-name>
  	<url-pattern>/EditStudent</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetStudentController</servlet-name>
  	<url-pattern>/GetStudent</url-pattern>
  </servlet-mapping>
</web-app>
